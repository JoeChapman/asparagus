{
    "predef": [
        "before",
        "afterAll",
        "beforeEach",
        "afterEach",
        "before",
        "after",
        "describe",
        "xdescribe",
        "it",
        "xit",
        "sandbox",
        "expect",
        "should",
        "window",
        "sinon",
        "mocha",
        "chai",
        "$"
